# matches the date
COLOR brightblack
{
(\[(Mon|Tue|Wed|Thu|Fri|Sat|Sun).(Jan|Feb|Mar|Apr|May|Jun|Jul|Aug|Sep|Oct|Nov|Dec).([0-9]{2})).*([0-9]{4}\])
}

# matches the time
COLOR blue
{
([0-9]{2}:[0-9]{2}:[0-9]{2})
}

# matches errors
COLOR red
{
(error)
}

# matches ip adresses
COLOR yellow
{
([0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3})
}